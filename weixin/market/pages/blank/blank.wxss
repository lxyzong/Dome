/* pages/blank/blank.wxss */

page {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.lnvite {
  width: 100%;
  height: 100%;
  background: white;
  display: flex;
  justify-content: center;
  align-items: center;
}

.lnvite_main {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.lnvite_main view {
  width: 336rpx;
  height: 96rpx;
  font-size: 28rpx;
  color: #666;
  letter-spacing: 0;
  text-align: center;
  line-height: 48rpx;
}

.lnvite_main image {
  width: 140rpx;
  height: 240rpx;
  margin-bottom: 60rpx;
}
